# Variáveis de produção para Coolify
# COPIE EXATAMENTE COMO ESTÁ E ALTERE APENAS OS VALORES COM ***

NODE_ENV=production
PORT=8000

# Banco de dados (copiar URL do PostgreSQL no Coolify)
DATABASE_URL="postgresql://***usuario***:***senha***@***host***:5432/kaosekai_db?schema=public"

# JWT - GERAR COM: openssl rand -base64 32
JWT_SECRET="***gerar-chave-forte-aqui***"
JWT_EXPIRES_IN="30d"

# CORS - seu domínio com wildcard
CORS_ORIGIN="https://app.seudominio.com,https://api.seudominio.com"
